<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <!-- launch the simulated environment -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find topological_swarm)/worlds/connected.world"/>
